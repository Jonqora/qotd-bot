{
  "name": "qotd-bot",
  "version": "1.0.0",
  "description": "Discord Bot for the leetcode_reddit question of the day.",
  "private": true,
  "engines": {
    "node": ">=16.6"
  },
  "scripts": {
    "build": "rimraf built && npm run lint && npm run prettier && tsc",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write .",
    "start": "node built/index.js"
  },
  "keywords": [
    "discord",
    "leetcode_reddit"
  ],
  "author": "Rafael Negron",
  "license": "ISC",
  "dependencies": {
    "discord.js": "13.6.0",
    "graphql": "^16.3.0",
    "graphql-request": "4.0.0",
    "node-cron": "3.0.0",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@types/node-cron": "3.0.1",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "typescript": "4.6.2"
  }
}
